{
  "name": "chapin",
  "version": "0.0.1",
  "private": true,
  "reactNativePermissionsIOS": [
    "Notifications"
  ],
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "set METRO_CONFIG=./metro.config.js && react-native start --config ./metro.config.js --reset-cache",
    "test": "jest",
    "clean-cache": "watchman watch-del-all && rm -rf $TMPDIR/react-* && rm -rf $TMPDIR/metro-* && rm -rf $TMPDIR/haste-map-* && rm -rf node_modules/.cache && yarn start --reset-cache",
    "pod-install": "cd ios && pod install",
    "pod-clean": "cd ios && pod deintegrate && pod clean && rm -rf Podfile.lock && pod install",
    "build:android": "cd android && ./gradlew bundleRelease",
    "build:android:apk": "npx react-native build-android --mode=release",
    "increment-version": "cd android && ./gradlew bundleRelease -PversionCode=$(($(grep 'versionCode' app/build.gradle | grep -o '[0-9]*') + 1))"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-firebase/analytics": "23.5.0",
    "@react-native-firebase/app": "23.5.0",
    "@react-native-firebase/auth": "23.5.0",
    "@react-native-firebase/firestore": "23.5.0",
    "@react-native-firebase/messaging": "23.5.0",
    "@react-native-masked-view/masked-view": "^0.3.2",
    "@react-navigation/native": "^6.1.0",
    "@react-navigation/native-stack": "^6.9.0",
    "@react-navigation/stack": "^6.4.0",
    "axios": "^1.3.4",
    "fast-xml-parser": "^3.17.5",
    "iconv-lite": "^0.6.2",
    "moment": "^2.29.1",
    "react": "19.0.0",
    "react-native": "0.79.2",
    "react-native-copilot": "^3.3.3",
    "react-native-device-info": "^14.0.4",
    "react-native-dotenv": "^3.3.1",
    "react-native-gesture-handler": "^2.20.2",
    "react-native-google-mobile-ads": "^15.8.3",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-permissions": "^3.8.0",
    "react-native-reanimated": "^3.12.1",
    "react-native-render-html": "^4.2.4",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "4.16.0",
    "react-native-svg": "^15.15.1",
    "react-native-webview": "^13.15.0",
    "react-native-youtube-iframe": "^2.4.1",
    "yenv": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.28.5",
    "@babel/preset-react": "7.28.5",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/cli": "^11.3.7",
    "@react-native-community/cli-platform-android": "^11.3.7",
    "@react-native/babel-preset": "0.79.2",
    "@react-native/eslint-config": "0.79.2",
    "@react-native/metro-config": "0.79.2",
    "@react-native/typescript-config": "0.79.2",
    "@types/react": "^19.0.0",
    "babel-jest": "29.7.0",
    "eslint": "^8.19.0",
    "jest": "^29.7.0",
    "patch-package": "^8.0.1",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "2.8.8",
    "react-test-renderer": "19.0.0",
    "typescript": "^5.4.0"
  }
}
