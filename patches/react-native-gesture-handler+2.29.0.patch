+package com.swmansion.gesturehandler;
+
+import androidx.annotation.NonNull;
+import com.facebook.react.ReactPackage;
+import com.facebook.react.bridge.NativeModule;
+import com.facebook.react.bridge.ReactApplicationContext;
+import com.facebook.react.uimanager.ViewManager;
+import java.util.Collections;
+import java.util.ArrayList;
+import java.util.List;
+
+public class RNGestureHandlerPackage implements ReactPackage {
+
+  @NonNull
+  @Override
+  public List<NativeModule> createNativeModules(@NonNull ReactApplicationContext reactContext) {
+    // No usamos RNGestureHandlerModule (Kotlin desactivado)
+    return Collections.emptyList();
+  }
+
+  @NonNull
+  @Override
+  public List<ViewManager> createViewManagers(@NonNull ReactApplicationContext reactContext) {
+    List<ViewManager> managers = new ArrayList<>();
+
+    // Intento 1: paquete m√°s com√∫n en RNGH 2.x
+    try {
+      Class<?> btn = Class.forName("com.swmansion.gesturehandler.react.RNGestureHandlerButtonViewManager");
+      managers.add((ViewManager) btn.getDeclaredConstructor().newInstance());
+    } catch (Throwable ignored) {}
+
+    try {
+      Class<?> root = Class.forName("com.swmansion.gesturehandler.react.RNGestureHandlerRootViewManager");
+      managers.add((ViewManager) root.getDeclaredConstructor().newInstance());
+    } catch (Throwable ignored) {}
+
+    // Intento 2: variantes posibles (por si el paquete/nombre difiere)
+    if (managers.isEmpty()) {
+      try {
+        Class<?> btn = Class.forName("com.swmansion.gesturehandler.RNGestureHandlerButtonViewManager");
+        managers.add((ViewManager) btn.getDeclaredConstructor().newInstance());
+      } catch (Throwable ignored) {}
+      try {
+        Class<?> root = Class.forName("com.swmansion.gesturehandler.RNGestureHandlerRootViewManager");
+        managers.add((ViewManager) root.getDeclaredConstructor().newInstance());
+      } catch (Throwable ignored) {}
+    }
+
+    return managers;
+  }
+}
